<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox id="backgroundPane_timer" fx:id="verticalPane" alignment="CENTER" prefHeight="250" prefWidth="500" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProjectMS.ResultScene.ResultSceneController">

    <HBox alignment="CENTER" prefHeight="80.0" prefWidth="300.0" spacing="50.0">
        <Button fx:id="toggleButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#toggleButtonpressed" prefHeight="75.0" prefWidth="300.0" style="-fx-background-color: transparent; -fx-cursor: hand;" text="Sound" textAlignment="CENTER" textFill="WHITE" underline="true">
            <graphic>
                <ImageView fitHeight="20.0" fitWidth="22.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="/ProjectMS/mute.png" />
                    </image>
                </ImageView>
            </graphic>
            <VBox.margin>
                <Insets left="480.0" top="20.0" />
            </VBox.margin>
            <font>
                <Font name="Arial" size="24.0" />
            </font></Button>
        <VBox.margin>
            <Insets top="100.0" />
        </VBox.margin>
    </HBox>
   <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="800.0" text="TIME IS GREATEST VALUE OF MAN" textAlignment="CENTER">
      <font>
         <Font name="Arial Italic" size="48.0" />
      </font>
      <VBox.margin>
         <Insets top="30.0" />
      </VBox.margin>
   </Text>
    <Label fx:id="TimeDisplay" textAlignment="CENTER" textFill="#f2f9ff" textOverrun="WORD_ELLIPSIS" VBox.vgrow="ALWAYS">
        <font>
            <Font name="Arial Black" size="150.0" />
        </font>
      <VBox.margin>
         <Insets top="80.0" />
      </VBox.margin>
    </Label>
    <Label fx:id="DateDisplay" textFill="#f2f9ff" textOverrun="WORD_ELLIPSIS" VBox.vgrow="ALWAYS">
        <font>
            <Font size="50.0" />
        </font>
      <VBox.margin>
         <Insets bottom="100.0" top="-30.0" />
      </VBox.margin>
    </Label>

    <HBox alignment="CENTER" prefHeight="150.0" prefWidth="817.0" spacing="50.0">
        <Button fx:id="StartButton" minHeight="-Infinity" mnemonicParsing="false" onAction="#startButtonPressed" opacity="1.0" prefHeight="80.0" prefWidth="300.0" style="-fx-background-color: rgba(54, 188, 132, 1); -fx-background-size: 10; -fx-background-radius: 10; -fx-cursor: hand;" text="Start" textAlignment="CENTER">
         <font>
            <Font name="Arial" size="30.0" />
         </font></Button>
      <Button fx:id="StopButton" layoutX="322.0" layoutY="10.0" mnemonicParsing="false" onAction="#stopButtonPressed" opacity="1.0" prefHeight="80.0" prefWidth="300.0" style="-fx-background-color: rgba(188, 183, 54, 1); -fx-background-size: 10; -fx-background-radius: 10; -fx-cursor: hand;" text="Stop" textAlignment="CENTER">
         <font>
            <Font name="Arial" size="30.0" />
         </font>
      </Button>
      <VBox.margin>
         <Insets />
      </VBox.margin>
    </HBox>
     <Button fx:id="MoveButton" alignment="CENTER" mnemonicParsing="false" onAction="#moveButtonPressed" opacity="1.0" prefHeight="43.0" prefWidth="259.0" style="-fx-background-color: transparent; -fx-cursor: hand;" text="&lt;&lt;&lt; GO TO CLOCK" textAlignment="CENTER" textFill="WHITE" underline="true">
      <VBox.margin>
         <Insets bottom="50.0" top="50.0" />
      </VBox.margin>
      <font>
         <Font name="Arial" size="24.0" />
      </font></Button>

</VBox>
